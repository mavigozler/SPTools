{
   "dev": {
      "createNonExistentPathsOnCopy": true,
      "rootpaths": {
         "projectRootDir": "D:/dev/SharePoint/SPTools",
         "remoteRootDir": "C:/Users/Mitch/California Department of Water Resources/Compliance Program Repository ME - Site Assets/SPTools",
      },
      "specialFiles" : {
         "lastCompileDateFileName": "lastCompileTime.txt"
      },
      "source": {
         "entryFile": {
            "sourcepath": "${rootpaths.projectRootDir}/src/SPTools.aspx",
            "destinationpath": "${rootpaths.remoteRootDir}",
            "variableValues": [
               { "variableName": "SPREST_JS_FolderPath", "value": "./js/SPREST" },
               { "variableName": "SPTOOLS_JS_FolderPath", "value": "./js" },
               { "variableName": "CSS_FolderPath", "value": "./css" }
            ]
         },
         "tsFiles": {
            "tsconfigJsonPath": "${rootpaths.projectRootDir}/tsconfig.json",
            "transpiledSameFolder": true,
            "pathsAndCopyTargets": [
               { "sourceRelativePath": "/src/ts", "copyTargetRelativePath": "" },
               { "sourceRelativePath": "/SPREST/src", "copyTargetRelativePath": "/SPREST" }
            ]
         },
         "otherSourceFiles": [
            {
               "path": "${rootpaths.projectRootDir}/src",
               "fName": "*.css",
               "localFinalCopyPath": "${rootpaths.projectRootDir}/out/css",
               "copyTargetRelativePath": "/css"
            },
            {
               "path": "${rootpaths.projectRootDir}/src",
               "fName": "*.aspx",
               "localFinalCopyPath": "${rootpaths.projectRootDir}/out",
               "copyTargetRelativePath": "."
            }
         ]
      },
      "backup": {
         "preBuildBackup": {
            "backupDestinationPath": "${rootpaths.remoteRootDir}/Backup",
            "items": [
               {
                  "from": "${rootpaths.projectRootDir}/build-tools/*.ps1",
                  "to": "${backup.preBuildBackup.backupDestinationPath}/build-tools"
               },
               {
                  "from": "${rootpaths.projectRootDir}/.vscode/*.json",
                  "to": "${backup.preBuildBackup.backupDestinationPath}/vscode"
               },
               {
                  "from": "${rootpaths.projectRootDir}/*.json",
                  "to": "${backup.preBuildBackup.backupDestinationPath}/projectFiles"
               }
            ]
         },
         "postBuildBackup": {
            "backupDestinationPath": "",
            "items": []
         }
      }
   },
